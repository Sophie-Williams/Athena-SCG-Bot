package gen;
import edu.neu.ccs.demeterf.lib.*;
import edu.neu.ccs.demeterf.*;

nogen List(X) = Cons(X) | Empty(X).
nogen Cons(X) = <first> X <rest> List(X).
nogen Empty(X) = .

SimpleProblems = 
  <constraints> List(Problem) EOF.
Problem = "problem" "(" 
  <constraints> List(Constraint) ")".
Constraint = "rel:" <r> Relation "vars:" <vars> List(Variable).
Relation = "or" <length> Length <pos> Positive.
Length = <v> int.
Positive = <v> int.
Variable = <v> ident.

